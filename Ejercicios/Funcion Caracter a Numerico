FUNCION carAEnt(var: AN(1)): N(1) // le doy un caracter y me devuelve un entero
	SEGUN var HACER
		"1": carAEnt := 1;
		"2": carAEnt := 2;
		"3": carAEnt := 3;
		"4": carAEnt := 4;
		"5": carAEnt := 5;
		"6": carAEnt := 6;
		"7": carAEnt := 7;
		"8": carAEnt := 8;
		"9": carAEnt := 9;
		"0": carAEnt := 0;
	FIN_SEGUN
FIN_FUNCION
