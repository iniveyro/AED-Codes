ACCION 3.22 ES
AMBIENTE
	A:ARREGLO [1...5, 1...5] DE ENTEROS;
	SAL:ARREGLO [1...2, 1...5] DE ENTEROS;
	I,J: ENTEROS;
	SUM1,SUM2: N(3);

PROCESO

	PARA I:= 1 A 2 HACER
		PARA J:= 1 A 5 HACER
			SAL[I,J]= 0;
		FINPARA
	FINPARA

	PARA I:=1 A 5 HACER
		PARA J:=1 A 5 HACER
			SAL[1,J] := SAL [1,J] + A[I,J]
			SAL[2,J] := SAL [2,J] + A[I,J]
		FINPARA
	FINPARA

FINACCION