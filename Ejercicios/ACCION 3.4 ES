ACCION 3.4 ES
AMBIENTE
	A: ARREGLO[1...50] DE ENTEROS;
	SAL: ARREGLO[1...50] DE ENTEROS;
	CANT: N(2);
	MULTI:N;
	I,J: ENTEROS;

PROCESO
	J:=1
	PARA I:=1 A 50 HACER
		
		MULTI:= A[I] MOD 3;
		
		SI (MULTI = 0) ENTONCES
			CANT:= CANT + 1;
		SINO
			SAL[J] := A[I]
			MIENTRAS (MULTI <> 0) HACER
				A[I]= A[I] + 1;
				MULTI:= A[I] MOD 3;
			FINMIENTRAS
			J:=J+1
		FINSI
	FINPARA
	ESC ('FUERON UN TOTAL DE:', CANT, 'LOS MULTIPLOS DE 3')
FINACCION